{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#app": [
        "./node_modules/nuxt3/dist/app/index"
      ]
    },
    "types": [
      "node",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/auth-next"
    ],
    "lib": ["esnext", "es2017", "dom"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
